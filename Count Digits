class Solution {
    static int evenlyDivides(int n) {
        // code here
        int count = 0;
        int num = n;
            while(n>0){
            int rem = n % 10;
            n = n/10;
            if(rem>0 &&num%rem==0){
                count++;
            }
            
        }
        return count;
    }
}
