class Solution {
    void pushZerosToEnd(int[] arr) {
        // code here
        int NumOfZero = 0;
        int i=0;
        for(int num:arr){
            if(num!=0){
                arr[i]=num;
                i++;
            }else{
                NumOfZero++;
            }
        }
        while(NumOfZero>0){
            arr[i]=0;
            i++;
            NumOfZero--;
        }
        
    }
}-
