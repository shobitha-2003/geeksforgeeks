class Solution {

    static int findFloor(int[] arr, int k) {
        // write code here
        int i=0;
        while(i<arr.length){
            if(arr[i]==k){
                return i;
            }else if(arr[i]>k){
                return i-1;
            }
            i++;
        }
        return -1;
    }
}
