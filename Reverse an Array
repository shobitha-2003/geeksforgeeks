class Solution {
    public void reverseArray(int arr[]) {
        // code here
        int len = arr.length;
        int num = len/2;
        int i = 0;
        int value = 0;
        while(num>0){
            value = arr[i];
            arr[i] = arr[len-1];
            arr[len-1] = value;
            i++;
            len--;
            num--;
        }
    }
}
